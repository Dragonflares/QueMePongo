            }
            li {
                float: left;
            }
            li a {
                display: block;
                color: black;
                text-align: center;
                padding: 16px;
                width: 215px;
                text-decoration: none;
            }
            li a:hover {
                width: 215px;
                background-color: #ffe6bc;  
            }
            table {
                width:100%;
            }
            table, th{
                border-collapse: collapse;
            }
            td {
                border-collapse: collapse;
            }
            th, td {
                padding: 15px;
                text-align: left;
            }
            table#t01 tr:nth-child(even) {
                background-color: rgb(255, 226, 177, 0.5);
            }
            table#t01 tr:nth-child(odd) {
                background-color: rgb(255, 214, 144, 0.5);
            }
            table#t01 th {
                background-color: rgb(255, 199, 102, 0.5);
                color: black;
            }
			
			
			
	
